{
  "name": "speed-percentile",
  "version": "1.3.0",
  "description": "Utility to compute speed percentile from sparse histogram hash.",
  "license": "ISC",
  "main": "lib/percentile.js",
  "repository": "https://github.com/mapbox/speed-percentile/",
  "engines": {
    "node": "4.2.2"
  },
  "scripts": {
    "test": "npm run lint; tap -R spec test/*.test.js;",
    "lint": "eslint *.js test/*.js lib/*.js",
    "fix": "eslint --fix *.js test/*.js",
    "coverage": "tap test/test.*.js --cov --coverage-report=lcov"
  },
  "devDependencies": {
    "eslint": "^2.11.1",
    "eslint-config-mourner": "^2.0.1",
    "tap": "^5.7.2"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "strict": 0,
      "indent": [
        2,
        2
      ],
      "camelcase": 0,
      "space-before-function-paren": [
        2,
        "never"
      ]
    }
  }
}
